      <pou name="complex" pouType="functionBlock">
        <interface>
          <localVars>
            <variable name="IN">
              <type>
                <INT/>
              </type>
            </variable>
            <variable name="N">
              <type>
                <BYTE/>
              </type>
            </variable>
            <variable name="OUT">
              <type>
                <INT/>
              </type>
            </variable>
            <variable name="ERR">
              <type>
                <BYTE/>
              </type>
            </variable>
            <variable name="I">
              <type>
                <INT/>
              </type>
            </variable>
            <variable name="a">
              <type>
                <INT/>
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[IF N > 1 AND N < 12 THEN
    ERR:=0;
    (* IF IN < P[0].X THEN *)
    IF IN < a THEN
      ERR := 2;
      OUT := a;
    (* ELSIF IN > P[N-1].X THEN *)
    ELSIF IN > a THEN
      ERR := 2;
      OUT := a;
    ELSE
      FOR I:=1 TO a-1 DO
        (* IF P[I-1].X >= P[I].X THEN *)
        IF a >= a THEN
          ERR:=1;
          EXIT;
        END_IF;
        (* IF IN <= P[I].X THEN *)
        IF IN <= a THEN
          EXIT;
        END_IF;
      END_FOR;
      IF ERR = 0 THEN
        (* OUT := DINT_TO_INT(P[I].Y - (P[I].X-IN) * (P[I].Y-P[I-1].Y) / (P[I].X-P[I-1].X)); *)
        OUT := (a - (a - IN) * (a - a) / (a - a));
      ELSE
        OUT:=0;
      END_IF;
    END_IF;
  ELSE
    ERR := 4;
  END_IF;]]></xhtml:p>
          </ST>
        </body>
      </pou>
